  }


  double sum = 0;
  for(unsigned int i=0; i<gretina->Size(); i++) {
    sum += gretina->GetGretinaHit(i).GetDoppler(GValue::Value("BETA"));
  }
  obj.FillHistogram(dirname, Form("calorimeter_%s_%s", incoming->GetName(), outgoing->GetName()),
                    2500, 0, 10000, sum);
  
